PyObject *MAKE_LIST{{args_count}}(
{% for i in range(args_count) %}
{% if i != 0 %}
    ,
{% endif %}
    PyObject *arg{{i}}
{% endfor %}
) {

    PyObject *result = MAKE_LIST_EMPTY({{args_count}});

    if (unlikely(result == NULL)) {
        return NULL;
    }

{% for i in range(args_count) %}
    CHECK_OBJECT(arg{{i}});
    Py_INCREF(arg{{i}});
    PyList_SET_ITEM(result, {{i}}, arg{{i}});

{% endfor %}

    return result;
}

