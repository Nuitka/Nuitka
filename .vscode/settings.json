        "clangcl",
        "clcache",
        "closefd",
        "co_argcount",
        "co_firstlineno",
        "co_kwonlyargcount",
        "co_posonlyargcount",
        "CO_VARKEYWORDS",
        "co_varnames",
        "compactify",
        "conda",
